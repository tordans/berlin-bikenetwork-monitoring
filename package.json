{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "type-check": "astro check && tsc --skipLibCheck --noEmit",
    "lint": "eslint '{src,keystatic}/**/*.+(ts|js|tsx|astro)' --fix",
    "format": "prettier --ignore-path .gitignore '{src,keystatic}/**/*.+(ts|js|tsx|astro)' --write",
    "format:all": "prettier --ignore-path .gitignore '{src,keystatic}/**/*.+(ts|js|tsx|astro|mdx)' --write",
    "check": "npm run type-check && npm run format && npm run lint",
    "updatePackages:major": "npx taze major --includeLocked --write && npm install && npm audit",
    "updatePackages:minor": "npx taze minor --includeLocked --write && npm install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/react": "^4.4.2",
    "@fontsource/hind": "^5.2.8",
    "@fontsource/open-sans": "^5.2.7",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@nanostores/react": "^1.0.0",
    "@nanostores/router": "^1.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@turf/turf": "^7.3.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "astro": "^5.16.6",
    "maplibre-gl": "^5.15.0",
    "nanostores": "^1.1.0",
    "pmtiles": "^4.3.0",
    "react": "^19.0.3",
    "react-dom": "^19.0.3",
    "react-map-gl": "8.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "prettier": "^3.7.4",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2"
  }
}
